{
  "name": "san-update",
  "version": "1.0.0",
  "description": "Scripts to immutably update object or array",
  "main": "index.min.js",
  "scripts": {
    "test": "edp test start --singleRun",
    "doc": "esdoc -c esdoc.json",
    "compile": "gulp",
    "clean": "gulp clean",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomfe/san-update.git"
  },
  "keywords": [
    "update",
    "immutable",
    "utility"
  ],
  "author": "otakustay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomfe/san-update/issues"
  },
  "homepage": "https://github.com/ecomfe/san-update#readme",
  "edp": {
    "main": "update.js",
    "wwwroot": "/",
    "depDir": "dep",
    "srcDir": "src",
    "loaderAutoConfig": "js,htm,html,tpl,vm,phtml",
    "loaderUrl": "http://s1.bdstatic.com/r/www/cache/ecom/esl/1-8-2/esl.js",
    "dependencies": {}
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "edp": "^1.1.1",
    "edp-test": "^1.1.1",
    "esdoc": "^0.4.4",
    "esdoc-es7-plugin": "0.0.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "rollup-stream": "^1.13.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
