{
  "name": "san-update",
  "version": "2.1.0",
  "description": "Scripts to immutably update object or array",
  "main": "index.min.js",
  "module": "index.es.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha",
    "test-cov": "npm run lint && cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov --reporter=html mocha",
    "report-cov": "cat coverage/lcov.info | coveralls",
    "ci": "npm run test-cov && npm run compile",
    "lint": "fecs",
    "doc": "esdoc -c esdoc.json",
    "compile": "gulp",
    "clean": "gulp clean",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecomfe/san-update.git"
  },
  "keywords": [
    "update",
    "immutable",
    "utility"
  ],
  "author": "otakustay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ecomfe/san-update/issues"
  },
  "homepage": "https://github.com/ecomfe/san-update#readme",
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "0.0.3",
    "fecs": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "2.2.0",
    "gulp-uglify": "^2.0.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-stream": "^1.14.0",
    "type-detect": "^4.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {}
}
